function calc(operation, a, b,) {
    switch (operation) {
        case 'add' :
            return a + b;
        case 'multi' :
            return a*b;
        case 'subtract' :
            return a - b;
    }
}

console.log (calc ('add', 15, 20));
console.log (calc ('multi', 10, 2));
console.log (calc ('subtract', 12, 11));
